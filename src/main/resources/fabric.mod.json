{
  "schemaVersion": 1,
  "id": "livechat2mc",
  "version": "${version}",
  "name": "LiveChat2MC",
  "description": "Brings YouTube Live Chat directly into your Minecraft game.",
  "environment": "client",
  "entrypoints": {
    "client": [
      "com.ytchatbridge.client.YTChatBridgeClient"
    ],
    "modmenu": [
      "com.ytchatbridge.client.ui.ModMenuEntry"
    ]
  },
  "depends": {
    "fabricloader": ">=${loader_version}",
    "minecraft": "${minecraft_version}",
    "fabric-api": ">=${api_version}"
  },
  "recommends": {
    "modmenu": ">=${modmenu_version}",
    "cloth-config": ">=${cloth_version}"
  },
  "mixins": [],
  "authors": [ "FlixZer" ],
  "contact": { "homepage": "https://discord.com/invite/cpyXYZArgm" },
  "license": "MIT"
  ,"icon": "assets/livechat2mc/icon.png"
}
